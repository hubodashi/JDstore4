<!-- 图片滚动 -->
<div class="container">
  <div class="row box">
      <div id="myCarousel" class="carousel slide">
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
     </ol>
     <div class="carousel-inner">
        <div class="item active">
         <a href="#">
          <img src="https://ws1.sinaimg.cn/large/006tNbRwly1fg4kj1ww00j30x30izn3v.jpg"  alt="First slide">
          <div class="carousel-caption"></div>
         </a>
        </div>
        <div class="item">
         <a href="#">
          <img src="https://ws1.sinaimg.cn/large/006tNbRwly1fg4kj1nm6pj30x30iz10s.jpg"  alt="Second slide">
          <div class="carousel-caption"></div>
         </a>
        </div>
        <div class="item">
          <a href="#">
            <img src="https://ws3.sinaimg.cn/large/006tNbRwgy1ffy1bww1yhj31hc0ukwud.jpg"  alt="Third slide">
            <div class="carousel-caption"></div>
          </a>
        </div>
      </div>

     <a class="carousel-control left" href="#myCarousel" data-slide="prev">
       <span class="glyphicon glyphicon-chevron-left"></span>
     </a>
     <a class="carousel-control right" href="#myCarousel" data-slide="next">
       <span class="glyphicon glyphicon-chevron-right"></span>
     </a>
      </div>
    </div>
</div>
